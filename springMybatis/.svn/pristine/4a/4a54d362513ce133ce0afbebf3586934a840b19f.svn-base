/*图标字体*/

@font-face {
  font-family: 'iconfont';  /* project id 214633 */
  src: url('//at.alicdn.com/t/font_yfxul8hoktpdgqfr.eot');
  src: url('//at.alicdn.com/t/font_yfxul8hoktpdgqfr.eot?#iefix') format('embedded-opentype'),
  url('//at.alicdn.com/t/font_yfxul8hoktpdgqfr.woff') format('woff'),
  url('//at.alicdn.com/t/font_yfxul8hoktpdgqfr.ttf') format('truetype'),
  url('//at.alicdn.com/t/font_yfxul8hoktpdgqfr.svg#iconfont') format('svg');
}

.iconfont{

     font-family: "iconfont";
    font-size: 16px;
    font-style: normal;
    margin-left: 14px;
    -webkit-font-smoothing: antialiased;
    -webkit-text-stroke-width: 0.2px;
    -moz-osx-font-smoothing: grayscale;
    display: inline-block;
    padding: 14px 0;
    color:white;
}

.text{
	display: inline-block;
