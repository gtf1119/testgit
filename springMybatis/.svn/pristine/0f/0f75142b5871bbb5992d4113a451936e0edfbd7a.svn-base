SELECT G.ID,G.NAME,G.PRICE,G.NUM,G.SALES,G.EVALUATENUM,G.DES,TO_CHAR(G.CREATETIME,'yyyy-MM--DD hh24:mi:ss') AS CREATETIME,U.NAME AS USERNAME,G.USER_ID,G.TYPE_ID,T.NAME AS TYPENAME FROM T_GOODS G INNER JOIN T_TYPE T ON G.TYPE_ID = T.ID INNER JOIN T_USER U ON G.USER_ID = U.ID; 


SELECT T1.ID,T1.GOODSNAME,T1.PRICE,T1.NUM,T1.SALES,T1.EVALUATENUM,T1.DES,T1.CREATETIME,T1.USERNAME,T1.USER_ID,T1.TYPE_ID,T1.TYPENAME,ROWNUM FROM 
(SELECT G.ID,G.NAME AS GOODSNAME,G.PRICE,G.NUM,G.SALES,G.EVALUATENUM,G.DES,TO_CHAR(G.CREATETIME,'yyyy-MM--DD hh24:mi:ss') AS CREATETIME,U.NAME AS USERNAME,G.USER_ID,G.TYPE_ID,T.NAME AS TYPENAME FROM T_GOODS G INNER JOIN T_TYPE T ON G.TYPE_ID = T.ID INNER JOIN T_USER U ON G.USER_ID = U.ID) T1;


SELECT T2.ID,T2.GOODSNAME,T2.PRICE,T2.NUM,T2.SALES,T2.EVALUATENUM,T2.DES,T2.CREATETIME,T2.USERNAME,T2.USER_ID,T2.TYPE_ID,T2.TYPENAME FROM (
SELECT T1.ID,T1.GOODSNAME,T1.PRICE,T1.NUM,T1.SALES,T1.EVALUATENUM,T1.DES,T1.CREATETIME,T1.USERNAME,T1.USER_ID,T1.TYPE_ID,T1.TYPENAME,ROWNUM AS RN FROM 
(SELECT G.ID,G.NAME AS GOODSNAME,G.PRICE,G.NUM,G.SALES,G.EVALUATENUM,G.DES,TO_CHAR(G.CREATETIME,'yyyy-MM--DD hh24:mi:ss') AS CREATETIME,U.NAME AS USERNAME,G.USER_ID,G.TYPE_ID,T.NAME AS TYPENAME FROM T_GOODS G INNER JOIN T_TYPE T ON G.TYPE_ID = T.ID INNER JOIN T_USER U ON G.USER_ID = U.ID) T1) T2
WHERE T2.RN > 0 AND T2.RN<=5


SELECT T3.ID,T3.GOODSNAME,T3.PRICE,T3.NUM,T3.SALES,T3.EVALUATENUM,T3.DES,T3.CREATETIME,T3.USERNAME,T3.USER_ID,T3.TYPE_ID,T3.TYPENAME,I.ID AS IMGID,I.FILENAME AS FILENAME,I.CREATETIME AS IMGTIME,I.GOODS_ID FROM(
SELECT T2.ID,T2.GOODSNAME,T2.PRICE,T2.NUM,T2.SALES,T2.EVALUATENUM,T2.DES,T2.CREATETIME,T2.USERNAME,T2.USER_ID,T2.TYPE_ID,T2.TYPENAME FROM (
SELECT T1.ID,T1.GOODSNAME,T1.PRICE,T1.NUM,T1.SALES,T1.EVALUATENUM,T1.DES,T1.CREATETIME,T1.USERNAME,T1.USER_ID,T1.TYPE_ID,T1.TYPENAME,ROWNUM AS RN FROM 
(SELECT G.ID,G.NAME AS GOODSNAME,G.PRICE,G.NUM,G.SALES,G.EVALUATENUM,G.DES,TO_CHAR(G.CREATETIME,'yyyy-MM--DD hh24:mi:ss') AS CREATETIME,U.NAME AS USERNAME,G.USER_ID,G.TYPE_ID,T.NAME AS TYPENAME FROM T_GOODS G INNER JOIN T_TYPE T ON G.TYPE_ID = T.ID INNER JOIN T_USER U ON G.USER_ID = U.ID) T1) T2
WHERE T2.RN > 0 AND T2.RN<=5) T3
LEFT JOIN T_IMG I ON I.GOODS_ID = T3.ID




