package com.ggg.pojo;

import java.util.List;

public class GoodType {
	private String id;
	private String name;
	private String createtime;
	private List<TypeTitle> ttlist;
	
	public List<TypeTitle> getTtlist() {
		return ttlist;
	}
	public void setTtlist(List<TypeTitle> ttlist) {
		this.ttlist = ttlist;
	}
	public String getId() {
		return id;
	}
	public void setId(String id) {
		this.id = id;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getCreatetime() {
		return createtime;
	}
	public void setCreatetime(String createtime) {
		this.createtime = createtime;
	}
	public GoodType() {
		super();
	}
	
	
	
	public GoodType(String name) {
		super();
		this.name = name;
	}
	public GoodType(String id, String name) {
		super();
		this.id = id;
		this.name = name;
	}
	
	
	
	
}
